{
  "name": "oss-management-system",
  "version": "1.0.0",
  "description": "OSS management system with GitHub Actions and Dependency-Track integration",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint scripts/**/*.ts",
    "lint:fix": "eslint scripts/**/*.ts --fix",
    "format": "prettier --write \"scripts/**/*.ts\"",
    "format:check": "prettier --check \"scripts/**/*.ts\""
  },
  "keywords": [
    "oss",
    "sbom",
    "dependency-track",
    "github-actions"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "js-yaml": "^4.1.0",
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fast-check": "^3.15.0",
    "globals": "^15.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
